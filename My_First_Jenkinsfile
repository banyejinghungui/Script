pipeline {
      agent any

      stages {
          stage('Prepare Environment'){
            steps{
              sh 'echo "Preparing Environment"'
            }
          }

          stage('Code Analyse'){
            steps {
              sh 'echo "Run some lints"'
            }
          }

          stage('Unit Test'){
            steps {
              sh 'echo "Tests will back"'
            }
          }

          stage('Build'){
            steps {
              sh 'echo "Building..."'
              sh 'echo "Done! And Cleaning..."'
            }
          }
      }

}
